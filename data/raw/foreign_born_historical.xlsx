      setTimeout(function() {
        var $ = jQuery;
        $(document).ready(function() {

          // Helper function to create cookies
          function setCookie(cname, cvalue, exdays) {
            var d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            var expires = "expires="+d.toUTCString();
            document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
          }

          // Set cookie to remember that we've already seen and closed the modal
          function timerModalClosed() {
            setCookie("time_modal", 'closed', 30);
            ga('send', 'event', 'modal', 'closed', '', 1);
          }

          // Function that sets modal with delay
          function showModal() {
            // Check to see if modal window exists
            if ($('.time-modal-trigger').length) {
              // Initialize modaal
              $(".time-modal-trigger").modaal({
                after_close: timerModalClosed
              });

              // Fire modaal
              $('.time-modal-trigger').click();
              timerModalClosed();
            }
          };

          // Check to see if we have a time delay
          // Let's check cookies to see if we can show modal
          function checkCookie() {
            var gdpr = getCookie("cookieconsent_status");
            var timeModal = getCookie("time_modal");
            if (gdpr == "dismiss" && timeModal != "closed") {
                            setTimeout(showModal, 5000);
                          }
          };

          // Helper function to ready cookies
          function getCookie(cname) {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for(var i = 0; i < ca.length; i++) {
              var c = ca[i];
              while (c.charAt(0) == ' ') {
                c = c.substring(1);
              }
              if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
              }
            }
            return "";
          }

          checkCookie();
        });
      }, 1500)
    </script></div>
